    "from fastai.vision.all import *\n",
    "import gradio as gr\n",
    "\n",
    "\n",
    "categories = ['cat', 'dog']\n",
    "learn = load_learner('model.pkl')\n",
    "\n",
    "def is_cat(img):\n",
    "    pred,idx,probs = learn.predict(img)\n",
    "    return {'prediction': categories[idx], 'confidence': probs[idx].item()}\n",
    "\n"
   ]
